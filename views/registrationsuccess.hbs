<body>
    <h3>
        Sign Up Successful
    </h3>
    <button class="btn btn-success buttons" type="button" onclick="signin()"> Sign In </button>
    <button class="btn btn-primary buttons" type="button" onclick="signup()"> Sign Up Another User </button>

    <script>
        const signin = () => {
            window.location.href = "/";
        }
        const signup = () => {
            window.location.href = "/signup";
        }
    </script>
</body>
